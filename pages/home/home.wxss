/* pages/home/home.wxss */
.home {
  height: 100vh;
  background-color: #F6CE2A;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
}
.home .title {
  color: #8f620f;
  font-size: 40rpx;
  font-weight: lighter;
  text-align: center;
}
.home .list {
  margin-top: 50rpx;
  padding: 50rpx;
}
.home .list .item {
  background-color: #F6AB25;
  box-shadow: 6px 6px 14px -2px rgba(168,120,25,1);
  height: 200rpx;
  border-radius: 12rpx;
  background-position: top -70rpx right -80rpx;
  background-repeat: no-repeat;
  background-size: 260rpx;
  display: flex;
  align-items: center;
  padding-left: 70rpx;
}
.list .item + .item {
  margin-top: 24rpx;
}
.home .list .item.week {
  background-image:url('data:image/svg+xml;base64,');
}
.home .list .item.month {
  background-image:url('data:image/svg+xml;base64,');
}
.home .list .item.all {
  background-image:url('data:image/svg+xml;base64,');
}
.home .list .item .item_name {
  font-size: 38rpx;
  font-weight: lighter;
  color: #8f620f;
}
